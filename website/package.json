    "@monaco-editor/react": "^4.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "netlify-cli": "^2.65.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
