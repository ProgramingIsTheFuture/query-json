{
  "name": "query-json-website",
  "version": "0.0.0",
  "scripts": {
    "start": "run-p --aggregate-output dev serve",
    "dev": "webpack -w",
    "build": "webpack",
    "build-production": "webpack --mode=production",
    "serve": "webpack-dev-server",
