{
  "name": "query-json-website",
  "version": "0.0.0",
  "entry": "_build/default/website/Website.bc.js",
  "scripts": {
    "start": "run-p --aggregate-output js:build server",
    "js:build": "wait-on $npm_package_entry && esbuild $npm_package_entry --platform=browser --outfile=dist/js/bundle.js --bundle --define:process.env.NODE_ENV='\"development\"'",
    "js:build:production": "esbuild $npm_package_entry --outfile=dist/js/bundle.js --minify --bundle --platform=browser --define:process.env.NODE_ENV='\"production\"'",
    "server": "PORT=5000 live-server . --host=localhost",
    "build": "js:build",
    "build:prod": "js:build:production"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.1.3",
    "esbuild": "^0.10.2",
    "gh-pages": "^3.1.0",
    "live-server": "^1.2.1",
    "netlify-cli": "^2.65.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "wait-on": "^5.3.0"
  },
  "browser": {
    "fs": false,
    "tty": false,
    "constants": false,
    "child_process": false
  }
}
